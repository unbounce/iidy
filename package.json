    "@types/request-promise-native": "^1.0.15",
    "@types/sprintf-js": "^1.1.1",
    "@types/tmp": "0.0.33",
    "@types/tv4": "^1.2.29",
    "@types/winston": "^2.4.4",
    "@types/wrap-ansi": "^3.0.0",
    "@types/yargs": "^12.0.1",
    "chai": "^4.2.0",
    "intercept-stdout": "^0.1.2",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
