    "request-promise-native": "^1.0.5",
    "tmp": "0.0.33",
    "ts-md5": "^1.2.4",
    "tv4": "^1.3.0",
    "winston": "^3.1.0",
    "wrap-ansi": "^4.0.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/bluebird-global": "^3.5.9",
    "@types/chai": "^4.1.6",
    "@types/cli-color": "^0.3.29",
    "@types/dateformat": "^1.0.1",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/handlebars": "^4.0.39",
    "@types/inquirer": "0.0.43",
