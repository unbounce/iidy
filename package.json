{
  "name": "iidy",
  "version": "1.7.0",
  "description": "A tool for working with CloudFormation and yaml templates.",
  "main": "lib/cli.js",
  "bin": "bin/iidy",
  "scripts": {
    "prepare": "npm run build",
    "postinstall": "patch-package",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "dev-test-watch": "mocha-typescript-watch -p tsconfig.json lib/tests/_init.js lib/tests/**/*js",
    "lint": "tslint -p . src/*ts src/**/*ts",
    "pkg-binaries": "pkg --out-path dist -t node8-macos,node8-linux package.json",
    "test": "mocha lib/tests/_init.js lib/tests/**/*js",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "author": "Tavis Rudd <tavis@unbounce.com>",
  "repository": "https://github.com/unbounce/iidy",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "auto-changelog": "^1.13.0",
    "aws-sdk": "unbounce/aws-sdk-js#c28cd0e923149586778cc6ef5b756b28724bc18a",
    "bluebird": "^3.5.4",
    "cli-color": "^1.4.0",
    "dateformat": "^3.0.3",
    "didyoumean2": "^3.0.0",
    "escape-string-regexp": "^2.0.0",
    "handlebars": "^4.1.2",
    "inquirer": "^6.3.1",
    "jmespath": "0.15.0",
    "js-yaml": "^3.13.1",
    "laundry-cfn": "1.1.0",
    "lodash": "^4.17.11",
    "ntp-client": "^0.5.3",
    "ora": "^3.4.0",
    "project-name-generator": "^2.1.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "tmp": "^0.1.0",
    "ts-md5": "^1.2.4",
    "tv4": "^1.3.0",
    "winston": "^3.2.1",
    "wrap-ansi": "^5.1.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/bluebird-global": "^3.5.11",
    "@types/chai": "^4.1.7",
    "@types/cli-color": "^0.3.29",
    "@types/dateformat": "^3.0.0",
    "@types/inquirer": "^6.0.0",
    "@types/jmespath": "^0.15.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.5",
    "@types/pad": "^1.0.2",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.43",
    "@types/request-promise-native": "^1.0.16",
    "@types/sprintf-js": "^1.1.2",
    "@types/tmp": "^0.1.0",
    "@types/tv4": "^1.2.29",
    "@types/winston": "^2.4.4",
    "@types/wrap-ansi": "^3.0.0",
    "@types/yargs": "^12.0.1",
    "chai": "^4.2.0",
    "intercept-stdout": "^0.1.2",
    "mocha": "^6.1.4",
    "mocha-typescript": "^1.1.17",
    "patch-package": "^6.1.2",
    "pkg": "^4.3.7",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
