{
  "name": "iidy",
  "version": "1.6.2",
  "description": "A tool for working with CloudFormation and yaml templates.",
  "main": "lib/cli.js",
  "bin": "bin/iidy",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "dev-test-watch": "mocha-typescript-watch -p tsconfig.json lib/tests/_init.js lib/tests/**/*js",
    "lint": "tslint -p . src/*ts src/**/*ts",
    "pkg-binaries": "pkg --out-path dist -t node8-macos,node8-linux package.json",
    "test": "mocha lib/tests/_init.js lib/tests/**/*js"
  },
  "author": "Tavis Rudd <tavis@unbounce.com>",
  "repository": "https://github.com/unbounce/iidy",
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "aws-sdk": "^2.167.0",
    "bluebird": "^3.5.1",
    "cli-color": "^1.2.0",
    "dateformat": "^2.0.0",
    "escape-string-regexp": "^1.0.5",
    "handlebars": "^4.0.10",
    "inquirer": "^3.3.0",
    "jmespath": "0.15.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "ntp-client": "^0.5.3",
    "ora": "^1.3.0",
    "project-name-generator": "^2.1.4",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "tmp": "0.0.31",
    "ts-md5": "^1.2.2",
    "tv4": "^1.3.0",
    "winston": "^2.4.0",
    "wrap-ansi": "^3.0.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/escape-string-regexp": "0.0.32",
    "@types/bluebird": "^3.5.14",
    "@types/bluebird-global": "^3.5.3",
    "@types/chai": "^4.0.4",
    "@types/cli-color": "^0.3.29",
    "@types/dateformat": "^1.0.1",
    "@types/handlebars": "^4.0.36",
    "@types/inquirer": "0.0.35",
    "@types/jmespath": "^0.15.0",
    "@types/js-yaml": "^3.9.1",
    "@types/lodash": "^4.14.68",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.33",
    "@types/ora": "^1.3.0",
    "@types/pad": "^1.0.0",
    "@types/request": "0.0.46",
    "@types/request-promise": "^4.1.38",
    "@types/request-promise-native": "^1.0.7",
    "@types/sprintf-js": "0.0.27",
    "@types/tmp": "0.0.33",
    "@types/tv4": "^1.2.28",
    "@types/winston": "^2.3.6",
    "@types/wrap-ansi": "^2.0.14",
    "@types/yargs": "^8.0.1",
    "chai": "^4.1.2",
    "intercept-stdout": "^0.1.2",
    "mocha": "^3.5.3",
    "mocha-typescript": "^1.1.12",
    "pkg": "^4.2.6",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
