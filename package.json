{
  "name": "iidy",
  "version": "1.6.5",
  "description": "A tool for working with CloudFormation and yaml templates.",
  "main": "lib/cli.js",
  "bin": "bin/iidy",
  "scripts": {
    "prepare": "npm run build",
    "postinstall": "patch-package",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "dev-test-watch": "mocha-typescript-watch -p tsconfig.json lib/tests/_init.js lib/tests/**/*js",
    "lint": "tslint -p . src/*ts src/**/*ts",
    "pkg-binaries": "pkg --out-path dist -t node8-macos,node8-linux package.json",
    "test": "mocha lib/tests/_init.js lib/tests/**/*js"
  },
  "author": "Tavis Rudd <tavis@unbounce.com>",
  "repository": "https://github.com/unbounce/iidy",
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "aws-sdk": "^2.268.1",
    "bluebird": "^3.5.1",
    "cli-color": "^1.2.0",
    "dateformat": "^3.0.3",
    "escape-string-regexp": "^1.0.5",
    "handlebars": "^4.0.11",
    "inquirer": "^6.0.0",
    "jmespath": "0.15.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "ntp-client": "^0.5.3",
    "ora": "^2.1.0",
    "project-name-generator": "^2.1.5",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "tmp": "0.0.33",
    "ts-md5": "^1.2.4",
    "tv4": "^1.3.0",
    "winston": "^3.0.0",
    "wrap-ansi": "^3.0.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.21",
    "@types/bluebird-global": "^3.5.5",
    "@types/chai": "^4.1.4",
    "@types/cli-color": "^0.3.29",
    "@types/dateformat": "^1.0.1",
    "@types/escape-string-regexp": "0.0.32",
    "@types/handlebars": "^4.0.38",
    "@types/inquirer": "0.0.42",
    "@types/jmespath": "^0.15.0",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.4",
    "@types/node": "^9.6.22",
    "@types/ora": "^1.3.4",
    "@types/pad": "^1.0.1",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.41",
    "@types/request-promise-native": "^1.0.15",
    "@types/sprintf-js": "^1.1.0",
    "@types/tmp": "0.0.33",
    "@types/tv4": "^1.2.29",
    "@types/winston": "^2.3.9",
    "@types/wrap-ansi": "^3.0.0",
    "@types/yargs": "^8.0.3",
    "chai": "^4.1.2",
    "intercept-stdout": "^0.1.2",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.16",
    "patch-package": "^5.1.1",
    "pkg": "^4.3.3",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
