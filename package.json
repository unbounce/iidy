    "node": ">=10.0.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "aws-sdk": "unbounce/aws-sdk-js#ec823721322196e30af242a35af9a6f4401198d8",
    "bluebird": "^3.5.2",
    "cli-color": "^1.4.0",
    "cfn-lint": "^1.9.2",
    "dateformat": "^3.0.3",
    "didyoumean2": "^2.0.0",
    "escape-string-regexp": "^1.0.5",
    "handlebars": "^4.0.12",
    "inquirer": "^6.2.0",
    "jmespath": "0.15.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "ntp-client": "^0.5.3",
    "ora": "^3.0.0",
